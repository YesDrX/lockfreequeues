        {
            "name": "Debug compile (cppdbg)",
            "type": "cppdbg",
            "MIMode": "lldb",
            "request": "launch",
            "program": "/Users/elijahrutschman/Development/Nim/bin/nim",
            "args": [
                "c",
                "--nimcache:/Users/elijahrutschman/.nim/cache",
